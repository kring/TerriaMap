    parameters: {
        googleUrlShortenerKey: null,
        googleAnalyticsKey: null,
        googleAnalyticsOptions: null,

        // Log ConsoleAnalytics events to console
        // logToConsole: true,

        /* Text that appears at the bottom of the map */
